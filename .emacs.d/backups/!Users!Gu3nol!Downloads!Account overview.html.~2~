<script>
    var currentbrandName = "britishgas";
    var currentUcrn = "003604197148";
    if(currentUcrn=='null'){
        currentUcrn = '';
    }
     var curr_date = new Date();
     curr_date.setTime(curr_date.getTime() + (10 * 24 * 60 * 60 * 1000));
    if(getCookie('SE_Title') != '' && currentUcrn == '' && document.cookie.indexOf('is_wtp') == -1){
        deleteCookie('is_wtp');
        setCookie('is_wtp', 'Y', curr_date, '/');
    }else if(getCookie('SE_Title') != '' && currentUcrn != '' && document.cookie.indexOf('is_wtp') == -1){
        deleteCookie('is_wtp');
        setCookie('is_wtp', 'N', curr_date, '/');
    }
    $(document).ready(function(){
        displayLogoutMsg();
        if((getCookie('treatmentcountcallnotneeded') == '' || document.cookie.indexOf('treatmentcountcallnotneeded') == -1) && getCookie('SE_Title') != '' && currentbrandName== 'britishgas') {
            treatmentsCall_NS();
        }
    });

    function displayLogoutMsg() {
        if (getCookie('SE_Title') != '') {
            try{
            $("#logoutMsg").show();
			if(currentbrandName=='britishgas'){
            if(document.getElementById('mega-menu') || document.getElementById('minimal-header-menu')){
                $("#logoutMsg").addClass("logout");
            }else{
                $("#logoutMsg").removeClass("logout");
            }
}

            $("#loginregister").addClass("displaynone");
                $("#titleUserName").text(getCookie('SE_Title')+" "+getCookie('SE_Lastname'));
                if(currentbrandName=='britishgas'){
                var megaColumn = $("li.last-menu div.mega-column-new");
                if(!(megaColumn.eq(1).find('img').length)){$(megaColumn).eq(1).remove();}

                    $("li.last-menu div.mega-column div.mega-column-content ul li ").remove();
                    $("li.last-menu div.mega-column-new").css("height","auto");
                    $("li.last-menu div.mega-wrapper").css("height","auto");
                     var lastMegaMenuDivLength = $("li.last-menu div.mega-column").length  ;
                     var MegaMenuSize = 5;
                     if(getCookie('is_wtp') != 'Y'){
                         MegaMenuSize = 5;
                     }else{
                         MegaMenuSize = 3;
                     }
                     if(lastMegaMenuDivLength<MegaMenuSize){
                         for(index = 1;index<=(MegaMenuSize-lastMegaMenuDivLength);index++){
                            $("li.last-menu div.mega-column-new").before("<div class='mega-column'><div class='mega-column-content'><h3><a role='menuitem' href='#'>sample1</a></h3></div></div>");
                         }
                     }
                     if(getCookie('is_wtp') != 'Y'){
                    var newmenu1= '.last-menu ul li .mega-column div.mega-column-content h3 a';
                    $(newmenu1+':eq(0)').html("Update your details").attr({href:"/Self-Service/Personal-Details-Entry/",title: "Update your details"});
                    $(".last-menu ul li .mega-column div.mega-column-content h3").eq(0).after('<ul><li role="presentation"><a href="/youraccount/discover/homemove.html" title="Moving home" role="menuitem"  tabindex="-1">Moving home</a></li></ul>');
                    if(document.cookie.indexOf('treatmentcountcallnotneeded') == -1){
                        $(newmenu1).eq(1).html("View your messages").attr({href:"/Your_Account/Your-Messages/",title:"View your messages"});
                    }else{
                        var treatmentCount = getCookie('treatmentcountcallnotneeded');
                        if(treatmentCount!=''){
                            $(newmenu1).eq(1).html("View your messages ("+treatmentCount+")").attr({href:"/Your_Account/Your-Messages/",title:"View your messages"});
                        }else{
                            $(newmenu1).eq(1).html("View your messages (0)").attr({href:"/Your_Account/Your-Messages/",title:"View your messages"});
                        }
                    }
                    $(newmenu1+':eq(2)').html("Your Nectar points").attr({href:"/Loyalty/View-Rewards-Content/?loginCustomerContent=true", title: "Your Nectar points"});
                    $(newmenu1+':eq(3)').html("Discover your account").attr({href:"/youraccount/discover.html",title: "Discover your account"});
                    $(newmenu1+':eq(4)').html("Log out").attr({href:"/Logout/Logout-Succesful/",title: "Log out"});
                    }else{
                        var newmenu1= '.last-menu ul li .mega-column div.mega-column-content h3 a';
                        if(document.cookie.indexOf('treatmentcountcallnotneeded') == -1){
                            $(newmenu1).eq(0).html("View your messages").attr({href:"/Your_Account/Your-Messages/",title: "View your messages"});
                        }else{
                            var treatmentCount = getCookie('treatmentcountcallnotneeded');
                            if(treatmentCount!=''){
                                $(newmenu1).eq(0).html("View your messages ("+treatmentCount+")").attr({href:"/Your_Account/Your-Messages/",title: "View your messages"});
                            }else{
                                $(newmenu1).eq(0).html("View your messages (0)").attr({href:"/Your_Account/Your-Messages/",title: "View your messages"});
                            }
                        }
                        $(newmenu1+':eq(1)').html("Discover your account").attr({href:"/youraccount/discover.html",title: "Discover your account"});
                        $(newmenu1+':eq(2)').html("Log out").attr({href:"/Logout/Logout-Succesful/",title: "Log out"});
                    }

                }else{
				    var megaColumn = $("li.last-menu div.mega-column-new");
                    if(!(megaColumn.eq(1).find('img').length)){$(megaColumn).eq(1).remove();}
                    $("li.last-menu div.mega-column div.mega-column-content ul li ").remove();
                    $(".last-menu ul li div.mega-column").eq(2).remove();
                                        $(".last-menu ul li div.mega-column").eq(2).remove();
                     var lastMegaMenuDivLength = $("li.last-menu div.mega-column").length  ;
                    $("li.last-menu div.mega-column-new").css("height","auto");
                    $("li.last-menu div.mega-wrapper").css("height","auto");
                    var MegaMenuSize = 2;
                     if(getCookie('is_wtp') != 'Y'){
                         MegaMenuSize = 2;
                     }else{
                         MegaMenuSize = 1;
                         $(".last-menu ul li div.mega-column").eq(1).remove();
                     }
                    if(lastMegaMenuDivLength<MegaMenuSize){
                        for(index = 1;index<=(MegaMenuSize-lastMegaMenuDivLength);index++){
                            $("li.last-menu div.mega-column-new").before("<div class='mega-column'><div class='mega-column-content'><h3><a role='menuitem' href='#'>sample1</a></h3></div></div>");
                         }
                     }
                    var newmenu1= '.last-menu ul li .mega-column div.mega-column-content h3 a';
                    if(getCookie('is_wtp') != 'Y'){
                    $(newmenu1+':eq(0)').html("Update your details").attr({href:"/Self-Service/Personal-Details-Entry/",title: "Update your details"});

                    $(newmenu1+':eq(1)').html("Log out").attr({href:"/Logout/Logout-Succesful/",title: "Log out"});
                    }else{
                                            $(newmenu1+':eq(0)').html("Log out").attr({href:"/Logout/Logout-Succesful/",title: "Log out"});
                    }

                }
               }
            catch(exception){
             }
        }else{
        	if(document.getElementById('mega-menu') || document.getElementById('minimal-header-menu')){
        		if(document.getElementById('preRegistrationCompletionPage')){
                    $("#loginregister").addClass("displaynone");
                }else{
                    $("#loginregister").removeClass("displaynone");
                }
        	}
        }
    }
</script>
<script type="text/javascript">

    // Treatments call for Overview Page
    function treatmentsCall_NS() {
        xmlHttpkana = GetXmlHttpObject();
        if (xmlHttpkana == null) {
            alert("Your browser does not support AJAX!");
            return;
        }
        var milliseconds = new Date().getTime();
        var url = "/apps/britishgas/components/TreatmentCall/GET.servlet?timeStamp="
                + milliseconds;
        xmlHttpkana.onreadystatechange = stateChangedforTreatment_NS;
        xmlHttpkana.open("GET", url, true);
        xmlHttpkana.send(null);
    }


    function stateChangedforTreatment_NS() {

        if (xmlHttpkana.readyState == 4) {
           /* $("#loadingMsg").hide();
            $("#messageText").show();
            var responseText = xmlHttpkana.responseText;
            var userNames = "";
            $("#defaultName").text(userNames);
            var mailSize = xmlHttpkana.responseText +"";
            $("#messageCenter a").attr("title",mailSize);
            $("#mailSize").text(mailSize);  */
            var msgCount = xmlHttpkana.responseText;
                if(currentUcrn!=''){
              $(".last-menu ul li .mega-column div.mega-column-content h3 a").eq(1).html("View your messages ("+msgCount+")").attr("title","View your messages");
                }else{
                $(".last-menu ul li .mega-column div.mega-column-content h3 a").eq(0).html("View your messages ("+msgCount+")").attr("title","View your messages");
                }
                var date = new Date();
                date.setTime(date.getTime() + (10 * 24 * 60 * 60 * 1000));
                deleteCookie('treatmentcountcallnotneeded');
                setCookie('treatmentcountcallnotneeded', msgCount, date, '/');
    }
}

  function GetXmlHttpObject() {
        var xmlHttp = null;
        try {
            // Firefox, Opera 8.0+, Safari
            xmlHttp = new XMLHttpRequest();
        } catch (e) {
            // Internet Explorer
            try {
                xmlHttp = new ActiveXObject("Msxml2.XMLHTTP");
            } catch (e) {
                xmlHttp = new ActiveXObject("Microsoft.XMLHTTP");
            }
        }
        return xmlHttp;
    }
</script>









