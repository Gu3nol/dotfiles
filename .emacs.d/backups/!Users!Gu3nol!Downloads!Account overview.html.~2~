            <div class="parsys loginRewardPoint">








<script type="text/javascript">
var brandName='BRITISHGAS';




$(document).ready(function(){


       RewardsCall_NS();



});
function RewardsCall_NS() {
          var milliseconds = new Date().getTime();
          var nectarCard_prefix=98263000;

           $.ajax({
               type : "GET",
               url : "/apps/britishgas/components/LoyaltyCall/GET.servlet?timeStamp="+milliseconds,
               success : function(response) {

                  if(response.loyaltydetails.ioerror && response.loyaltydetails.ioerror!= undefined)
                   {
                       $(".ioerror").show();
                   }
                   if(response.loyaltydetails.serviceerror && response.loyaltydetails.serviceerror!= undefined)
                   {
                       $(".serviceerror").show();
                   }
                   if(response.loyaltydetails.NectarPointsGrtone && response.loyaltydetails.NectarPointsGrtone!= undefined)
                   {
                       if(brandName!='BRITISHGAS'){
                           $(".ptstotl-clctd").html(response.loyaltydetails.totalPoints);
                           if(response.loyaltydetails.necBalanceDate!='01-01-0001' && response.loyaltydetails.necBalanceDate!=null){
                           $(".ptstotl-baldate").html(response.loyaltydetails.necBalanceDate);
                           $(".ptstotl-necbal").html(response.loyaltydetails.nectarPointBalance);

                           $(".ptstotl-nectar").show();
                           }
                           $(".ptstotl-necnum").html(nectarCard_prefix);
                           var necCard=response.loyaltydetails.nectarNumber;
                           var maskednecNumber=necCard.substring(0,4)+"XXXX"+necCard.substring(8);
                           $(".ptstotl-necnum").append(maskednecNumber);
                           $(".NectarPointsGrtone-fusion").show();
                           }
                           else{
                               $(".NectarPointsGrtone").show();
                               }



                   }
                   if(response.loyaltydetails.NectarPointsEqOne && response.loyaltydetails.NectarPointsEqOne!= undefined)
                   {

                       if(brandName!='BRITISHGAS'){
                       $(".ptscllct-erndpts").html(response.loyaltydetails.pointsEarned);
                          if(response.loyaltydetails.necBalanceDate!='01-01-0001' && response.loyaltydetails.necBalanceDate!=null){
                       $(".ptscllct-baldate").html(response.loyaltydetails.necBalanceDate);
                       $(".ptscllct-necbal").html(response.loyaltydetails.nectarPointBalance);
                       $(".ptscllct-nectar").show();
                       }

                       $(".ptscllct-cardno").html(nectarCard_prefix);
                      var nectarCardNumber=response.loyaltydetails.nectarNumber;
                      var maskedNumber=nectarCardNumber.substring(0,4)+"XXXX"+nectarCardNumber.substring(8);
                       $(".ptscllct-cardno").append(maskedNumber);

                       $(".NectarPointsEqOne-fusion").show();

                       }
                       else{
                        $(".NectarPointsEqOne").show();
                           }
                   }
                   if(response.loyaltydetails.NectarPoints&&response.loyaltydetails.NectarPoints!= undefined)
                   {
                       $(".NectarPoints").show();
                   }

                     if(response.loyaltydetails.RegisterNow && response.loyaltydetails.RegisterNow != undefined)
                   {
                         if(brandName!='BRITISHGAS'){
                             $(".RegisterNow-fusion").show();
                         }
                         else{

                       $(".RegisterNow").show();

                         }
                   }
                   $(".Swan").hide();
               },
               error : function(e) {
                   //alert("RESULT ::: "+e);
               }
           });
}

</script>



