-- This VHDL code is for testing purpose

-- It is developed for an 12 switches selector
--------------------------------------------------------

LIBRARY IEEE;

USE IEEE.std_logic_1164.ALL;
USE IEEE.numeric_std.ALL;

ENTITY tb_mux IS
  PORT(
    -- Columns/Row signals
    switches : OUT std_logic_vector(11 DOWNTO 0);
    );
END tb_mux;
