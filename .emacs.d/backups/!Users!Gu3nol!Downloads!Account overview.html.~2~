<div class="siteSearchJourney parsys globalSiteSearch">














<div class="topsrch fleft">
    <form id="newSearchFormSmall" name="newSearchFormSmall" action="/siteSearch/sitesearchresults.html" method="get">
        <fieldset>
            <label for="searchtermsmall" class="displaynone">Search</label>
            <legend class="displaynone">Search</legend>
            <input aria-haspopup="true" aria-autocomplete="list" role="textbox" autocomplete="off" name="queryString" id="searchtermsmall" maxlength="" value="Search" class="srchbox fleft ui-autocomplete-input" aria-describedby="" aria-required="true" type="text">
             <!-- Go button -->
             <input value="Search" name="smallMediumPlaceholderH" id="smallMediumPlaceholderH" type="hidden">
             <input value="UTF-8" name="_charset_" id="_charset_" type="hidden">
            <button id="" type="submit" title="Search" class="newSearchButtonSmall fleft"> </button>
         </fieldset>
    </form>
</div>
<div class="cb"></div>
    <div id="searchtermsmallresult">
<ul style="z-index: 1; top: 0px; left: 0px; display: none;" aria-activedescendant="ui-active-menuitem" role="listbox" class="ui-autocomplete ui-menu ui-widget ui-widget-content ui-corner-all"></ul></div>
    <script type="text/javascript">

 $(document).ready(function(){
 var defaultMsgLarge = $("#largePlaceholderH").val();
 var defaultMsgSmallMedium = $("#smallMediumPlaceholderH").val();
 //console.log("defaultMsgLarge = "+ defaultMsgLarge);
 //console.log("defaultMsgSmallMedium = "+ defaultMsgSmallMedium );
    //small and medium search boxes
    $('#searchtermsmall, #searchtermmedium').click(function(e){
           if ($(this).val()==defaultMsgSmallMedium ) {
                $(this).val('');
            }
       });

    $('#searchtermsmall, #searchtermmedium').focus(function(e){
           if ($(this).val()==defaultMsgSmallMedium) {
               $(this).val('').css("color","black" );
               }
           return false;
    });

    $('#searchtermsmall, #searchtermmedium').blur(function(e){
           if ($(this).val()=='') {
               $(this).val(defaultMsgSmallMedium).css("color","#999999" );
               return false;
              }
       });

    //logic for the small submit btn
    $(".newSearchButtonSmall, .newSearchButtonSmallTablet").click(function(e){
       if ( ($("#searchtermsmall").val() == defaultMsgSmallMedium) || ($("#searchtermsmall").val() == ' ') )
           {
               e.preventDefault();
           }
    });

    //logic for the medium submit btn
     $("#siteSearchSubmitMedium").click(function(){
        if ( ($("#searchtermmedium").val() != '') && ($("#searchtermmedium").val() != defaultMsgSmallMedium) ){
           $("#searchtermsmall").val($("#searchtermmedium").val());
            $( "#newSearchFormSmall").submit();
           }
     });

   //logic for the large search box on h&a only
    if( $("input[name='defaultFilter']").val() == "HelpAndAdvice" ){
         if ( $('#searchtermlarge').val() == ''){
            $('#searchtermlarge').val(defaultMsgLarge).css("color","#999999" );
         }

         $('#searchtermlarge').click(function(e){
               if ($(this).val()=='defaultMsgLarge ') {
                    $(this).val('');
                }
           });

           $('#searchtermlarge').focus(function(e){
               if ($(this).val() == defaultMsgLarge) {
                   $(this).val('').css("color","black" );
                   }
               return false;
           });

        $('#searchtermlarge').blur(function(e){
               if ($(this).val()=='') {
                   $(this).val(defaultMsgLarge).css("color","#999999" );
                   return false;
                  }
           });
    }

    //logic for the large submit btn
    $("#siteSearchSubmit").click(function(e){
       if ( ($("#searchtermlarge").val() == '') || ($("#searchtermlarge").val() == defaultMsgLarge ) )
           {
              e.preventDefault();
           }
    });

   //logic for the filters
    if($("#entireSite").attr("checked")==true){
        $("#smartLivingH").val("");
        $("#productsAndServicesH").val("");
        $("#helpAndAdviceH").val("");
        $("#videosH").val("");
    }

    if($("#smartLiving").attr("checked")==true){
        $("#smartLivingH").val("SmartLiving");
    }

    if($("#productsAndServices").attr("checked")==true){
        $("#productsAndServicesH").val("ProductsAndService");
    }

    if($("#helpAndAdvice").attr("checked")==true){
        $("#helpAndAdviceH").val("HelpAndAdvice");
    }


    if($("#videos").attr("checked")==true){
        $("#videosH").val("video");
    }


   $("#entireSite").change(function() {

        enableDisableFilter($(this));
        $("#smartLivingH").val("");
        $("#productsAndServicesH").val("");
        $("#helpAndAdviceH").val("");
        $("#videosH").val("");
        updateFilters("entireSite", "entireSiteH");
        $("#newSearchFormId").submit();
    });

    $("#helpAndAdvice").change(function() {

        enableDisableFilter($(this));
        $("#entireSiteH").val("");
        $('#entireSite').attr('checked', false);
        updateFilters("helpAndAdvice", "helpAndAdviceH");
        $("#newSearchFormId").submit();
    });

    $("#smartLiving").change(function() {


        enableDisableFilter($(this));
        $("#entireSiteH").val("");
        $('#entireSite').attr('checked', false);
        updateFilters("smartLiving", "smartLivingH");
        $("#newSearchFormId").submit();
    });

    $("#productsAndServices").change(function() {


        enableDisableFilter($(this));
         $("#entireSiteH").val("");
              $('#entireSite').attr('checked', false);
           updateFilters("productsAndServices", "productsAndServicesH");
           $("#newSearchFormId").submit();
    });

    $("#videos").change(function() {


        enableDisableFilter($(this));
        $("#entireSiteH").val("");
        $('#entireSite').attr('checked', false);
        updateFilters("videos", "videosH");
        $("#newSearchFormId").submit();
    });
} );


    function updateFilters(checkID, hiddenID){



        if($("#"+checkID).attr("checked")==true)
        {
                $("#"+hiddenID).val($("#"+checkID).val());
        }
        else
        {
                $("#"+hiddenID).val("");
        }
    }

    function entireSiteCheck(){
        if ($("#helpAndAdvice").not(':checked') && $("#smartLiving").not(':checked') &&
                $("#productsAndServices").not(':checked') && $("#videos").not(':checked')) {
            $('#entireSite').attr('checked', true);
        }
    }

    function enableDisableFilter(checkbox) {
        if ($(checkbox).val() == '') {
            $('#helpAndAdvice').attr('checked', false);
            $('#smartLiving').attr('checked', false);
            $('#productsAndServices').attr('checked', false);
            $('#videos').attr('checked', false);
        } else if ($(checkbox).val() == 'HelpAndAdvice' || $(checkbox).val() == 'SmartLiving'
                    || $(checkbox).val() == 'ProductsAndService' || $(checkbox).val() == 'video') {
            $('#entireSite').attr('checked', false);
        }
    }
</script>




    </div>

