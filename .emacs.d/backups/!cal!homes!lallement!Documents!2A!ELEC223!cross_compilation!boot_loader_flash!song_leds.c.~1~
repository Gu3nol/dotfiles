#include "timer.h"
#include "buzzer.h"
#include "song.h"

int main(void) {
  /* Initialisation du port s√©rie */
  serial_init();
  serial_puts("\r\nLet's play some music...\r\n");

  /* Initilisation */
  // leds
  leds_init();

  // buzzer
  buzzer_init();
