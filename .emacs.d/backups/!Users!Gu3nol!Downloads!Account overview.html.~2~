<script>
$(document).ready(function() {
 checkForCookie();
$(".search-icon,button").click(function() {
    acceptCookie();
});

$("a").click(function() {
var anchorVariable=$(this).attr("href");
    if ((anchorVariable !== "javascript:;") && (anchorVariable !== "javascript:acceptCookie();")) {
    var isCookieSet1=getCookie('BG-COOKIE');
        if (isCookieSet1 === null || isCookieSet1 === "" || isCookieSet1 === undefined) {
            setTimeout(function() {
              checkForCookie();
               }, 5000);
        } else {
            acceptCookie();
        }
    }
});
$(".globalCookieMessage a").click(function() {
var anchorVariable=$(this).attr("href");
            if (anchorVariable === "#") {
                window.location.href='/content/britishgas/global-maintenance/cookies-policy.html';
                acceptCookie();
            } else {
                acceptCookie();
            }
    });
});

function checkForCookie(){
    var isCookieSet=getCookie('BG-COOKIE');
    if (isCookieSet === null || isCookieSet === "" || isCookieSet === undefined) {
         var date = new Date();
        date.setTime(date.getTime() + (100*24*60*60*1000));
        setCookie('BG-COOKIE','acceptcookie',date,'/');
        $(".bgrcookiemessage").removeClass("displaynone");
    } else{
          $(".bgrcookiemessage").addClass("displaynone");
    }
}

function acceptCookie(){
    var date = new Date();
    date.setTime(date.getTime() + (100*24*60*60*1000));
    setCookie('BG-COOKIE','acceptcookie',date,'/');
    $(".bgrcookiemessage").addClass("displaynone");
}
</script>
