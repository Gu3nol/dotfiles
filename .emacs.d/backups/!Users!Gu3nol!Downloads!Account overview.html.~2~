function eligibleToShowLoadingForSMU(){
var bgmoAcctCount = '';
var normalAcctCount = '';
var smartMeterStatus = 'ON';
var brand = 'britishgas';
var mpan = '';
var mprn = '';
var energyAccountCount = '';
var isEnergyOnlyCustomer = '';
if(smartMeterStatus == 'ON' && ((mpan != 'null' && mpan != '') || (mprn != '' && mprn != 'null')) && brand == 'britishgas'){
       if(($('#isSmartAcct').val() == 'true' && bgmoAcctCount == 0)
           || (normalAcctCount > 0 && (energyAccountCount <= normalAcctCount) && (bgmoAcctCount < normalAcctCount))
           || (normalAcctCount > 0 && (energyAccountCount < normalAcctCount)&& (bgmoAcctCount < normalAcctCount))){
     $("#smartMeterSectionLoading").show();
        getCustomerSmartMeterDetails_NS();
}else{
     $("#smartMeterSectionLoading").hide();
}
}
}
function getCustomerSmartMeterDetails_NS(){
var fromOverview = 'true';
var idValue = '';
var borderClass = '"section-container fleft panel-border-top"';
var imageIcon = '<div class="icon-cont fleft"><img class="icons" src="" alt="smartMeter" title="smartMeter"></div>';
var address1 = '';
var address2 = '';
var isRIStatus = '';
var ucrn = '003604197148';
var linkName = '';
var upgradeFreeSmartMeter = '';
var emptyBalanceSection = '<div id="smartUpgradeAccount" class="account-balance fleft"></div>';
var cstList = '';
var businessPartner = '';
var mpanAndAcctNumber = '';
var mprnAndAcctNumber = '';
var address = "";
var cstEligibleStatus = '';
var sectionDivID = '';
var onclickValue = '';
var linkSectionDiv = '';
var greatNesSection = '';
var greatNewsPara = "";
var splitAddressGrtNews = '';
var convertedAddress = '';
var whatsThisOverlayID = 'smartmeterupgradewhatsthisoverlay';
var whatsThisRegInterestOverlayID = 'smartmeterregisterinterestwhatsthisoverlay';
var overLay = 'overlay';
var appointmentAvailable = '';
var whatsThisSection = '<p class="default"><a class="tooltip" onclick="openModal('+"'"+whatsThisOverlayID+"'"+', '+"'"+overLay+"'"+', this);" href="javascript:void(0);" title="(What'+"'"+'s this?)">(What'+"'"+'s this?)</a></p>';
 var whatsThisSectionregister ='<p class="default"><a class="tooltip" onclick="openModal('+"'"+whatsThisRegInterestOverlayID+"'"+', '+"'"+overLay+"'"+', this);" href="javascript:void(0);" title="(What'+"'"+'s this?)">(What'+"'"+'s this?)</a></p>';
var tubeMapLinkSectionDiv = '';
var elecAcctNumber = '';
var tubeMapLinkName = 'Your smart journey';
var smartAcquiredCustomer = '';
var showLinkTubeMap = '';
var meterType = "";
     $.ajax({
          type : "POST",
          url: "/apps/britishgas/components/smartmeter/checksmarteligibility/POST.servlet",
          data : "isFromAcctOverview="+fromOverview,
          async:true,
          dataType : "json",
        success:function(response) {
        $.each(response.valid,function(key,value){

        if(key != '' && value != ''){
                idValue = key.replace(',','');
                cstList = value.toString().split(",");
                   if(cstList.length == 9){
                   $("#smartMeterSectionLoading").hide();
                        mpanAndAcctNumber = cstList[0]+","+cstList[1];
                        mprnAndAcctNumber = cstList[2]+","+cstList[3];
                        elecAcctNumber = cstList[1];
                        address = cstList[4];
                        cstEligibleStatus = cstList[5];
                         address1 = getAddressLine1(address);
                         appointmentAvailable = cstList[6];
                         meterType = cstList[7];
                         if(meterType === "") {
                         meterType = cstList[8];
                         }
                      splitAddressGrtNews = address.split('/');
                     convertedAddress =  splitAddressGrtNews[1].toLowerCase();
                   if(cstEligibleStatus == 'true'){
                   var slicedAddress = '';
                       sectionDivID = 'smartMeterSuccess';
                       linkName = '';
                       if(convertedAddress.slice(0,1) != ''){
                       slicedAddress = convertedAddress.slice(1,2).toUpperCase() + convertedAddress.slice(2);
                       }else{
                       slicedAddress = convertedAddress.slice(0,1).toUpperCase() + convertedAddress.slice(1);
                       }
                       greatNesSection  = '<div class="blue-promo" style="width:96%" ><div class="info"><div class="parsys panel-content"><div class="text parbase section"><h2><b><span class="large"></span> &ndash;</b>  '+splitAddressGrtNews[0]+','+slicedAddress+'.</h2><p><br>'+greatNewsPara.replace("'",'"')+'</p></div></div></div></div>';
                       onclickValue = "redirectToBGMOPage('"+mpanAndAcctNumber+"','"+mprnAndAcctNumber+"','"+ucrn+"','"+address+"','?meterType=" + meterType +" ','accountsOverviewForm','"+appointmentAvailable+"');";
                      if(smartAcquiredCustomer != null && smartAcquiredCustomer !='' && smartAcquiredCustomer == 'true' ){
                       tubeMapLinkSectionDiv = '<a class="fourth-level-link fleft" title="'+tubeMapLinkName+'" href="javascript:void(0);" onClick="redirectToTubeMapPage('+elecAcctNumber+');">'+tubeMapLinkName+'</a>';
                       }else if(showLinkTubeMap != null && showLinkTubeMap != '' && showLinkTubeMap == 'true'){
                           tubeMapLinkSectionDiv = '<a class="fourth-level-link fleft" title="'+tubeMapLinkName+'" href="javascript:void(0);" onClick="redirectToTubeMapPage('+elecAcctNumber+');">'+tubeMapLinkName+'</a>';
                       }
                   linkSectionDiv = '<div class="account-link-smart fleft"><a class="fourth-level-link fleft" title="'+linkName+'" href="javascript:void(0);" onClick="'+onclickValue+'">'+linkName+'</a>'+tubeMapLinkSectionDiv+'</div>';
                 }else if(cstEligibleStatus == 'false'){
                   greatNesSection = '';
                       sectionDivID = 'smartMeterFailure';
                       if(isRIStatus == 'false'){
                       linkName = '';
                       }else{
                       linkName = '';
                       }
                       onclickValue = "redirectToRegisterYourInterestPage('"+mpanAndAcctNumber+"','"+mprnAndAcctNumber+"','"+ucrn+"','?meterType=" + meterType +"','accountsOverviewForm','');";
                     linkSectionDiv = '<div class="account-link-smart fleft"><a class="fourth-level-link fleft" title="'+linkName+'" href="javascript:void(0);" onClick="'+onclickValue+'">'+linkName+'</a></div>';
                   }
                var smartMeterHeading = '<div class="cust-info fleft"><div id="address'+idValue+'" class="heading"><h3></h3></div>';

                $("#"+sectionDivID).append('<div id='+idValue+' class='+borderClass+'></div>');
                $("#"+idValue).append(greatNesSection);
                $("#"+idValue).append(imageIcon);
                $("#"+idValue).append(smartMeterHeading);
                $("#address"+idValue).append('<p class="small">'+address1+'</p>');
               if(cstEligibleStatus == 'true'){
                $("#address"+idValue).append(whatsThisSection);
                }else{
                    $("#address"+idValue).append(whatsThisSectionregister);
                }
                $("#"+idValue).append(emptyBalanceSection);
                $("#"+idValue).append(linkSectionDiv);
                }
              if(cstList.length == 8){
              greatNesSection = '';
                   $("#smartMeterSectionLoading").hide();
                        if(cstList[0] != ''){
                            mpanAndAcctNumber = cstList[0]+","+cstList[1];
                        }else{
                            mprnAndAcctNumber = cstList[1]+","+cstList[2];
                        }
                        address = cstList[3];
                        address1 = getAddressLine1(address);
                        cstEligibleStatus = cstList[4];
                         meterType = cstList[6];
                         if(meterType === "") {
                         meterType = cstList[7];
                         }
                   if(cstEligibleStatus == 'false'){
                      sectionDivID = 'smartMeterFailure';
                       if(isRIStatus == 'false'){
                       linkName = '';
                       }else{
                       linkName = '';
                       }
                       onclickValue = "redirectToRegisterYourInterestPage('"+mpanAndAcctNumber+"','"+mprnAndAcctNumber+"','"+ucrn+"','?meterType=" + meterType +" ','accountsOverviewForm','');";
                   }
                var smartMeterHeading = '<div class="cust-info fleft"><div id="address'+idValue+'" class="heading"><h3></h3></div>';
                linkSectionDiv = '<div class="account-link-smart fleft"><a class="fourth-level-link fleft" title="'+linkName+'" href="javascript:void(0);" onClick="'+onclickValue+'">'+linkName+'</a></div>';
                $("#"+sectionDivID).append('<div id='+idValue+' class='+borderClass+'></div>');
                $("#"+idValue).append(greatNesSection);
                $("#"+idValue).append(imageIcon);
                $("#"+idValue).append(smartMeterHeading);
                $("#address"+idValue).append('<p class="small">'+address1+'</p>');
                if(cstEligibleStatus == 'true'){
                $("#address"+idValue).append(whatsThisSection);
                }else{
                    $("#address"+idValue).append(whatsThisSectionregister);
                }
                $("#"+idValue).append(emptyBalanceSection);
                $("#"+idValue).append(linkSectionDiv);
                }
            }
        });
            $("#smartMeterSectionLoading").hide();
        },
        error: function(e) {
       $("#smartMeterSectionLoading").hide();
       }
    });
}
function getAddressLine1(address){
var addressArray = '';
var addressOne = ''
var addressTwo = '';
var finalAdress = '';
var lengthCount  = 0;

var i=3;
if(address != null || address != ''){
    addressArray = address.split("/");
    lengthCount = address.split("/").length;
    if(lengthCount > 3){
            addressOne = addressArray[0]+","+addressArray[1]+","+addressArray[2]+",";
        for(var j=i;j<lengthCount;j++){
            addressTwo = addressTwo+addressArray[j];
        }



finalAdress = addressOne+"<br>"+addressTwo+".";


        return finalAdress;
    }
}
return addressArray;
}
