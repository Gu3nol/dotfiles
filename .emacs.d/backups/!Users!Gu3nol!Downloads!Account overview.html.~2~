<script>
var exvalues= {},queryStr,contentId,contractNum,currentPosit;
var totalproperties=$('#no-ofproperties').val();
var demoTemplate;
var rangeofPaginate=10;
var isFreeze=false;
var urlpath="/etc/centrica/errorMessages/_jcr_content/";
   $(document).ready(function(){
          $('#findIDOAM').click(function(event){findAccount();})
   });
function findAccount(){
      if(isFreeze==true){
          freezeAccountDetailsCall();
      }
    else
          validateAccountNumber();
}
function showOverlay() {
    isFreeze=false;
    $('#missingAccountError').removeClass('error').html('');
    $('#missingAccountError, #missingAccountInfo').hide();
    $("#missingAccount").removeClass("error-field");
       $("#missingAccountDiv .customerReferenceNumber").show();
       $("#missingAccount").val('');
}
   function validateAccountNumber() {
      $('#missingAccountError').removeClass('error').html('');
      $('#missingAccountError, #missingAccountInfo').hide();
      $("#missingAccount").removeClass("error-field");
        var accountnumber = $('#missingAccount').val();
        var errorLabel=$.trim($("label[for='missingAccount']").html());
        var errormessage = "";
        errormessage= "<h3 class='error-text'>"+getErrorMessage('error.page.no.account.found.json')+"</h3>";//"Sorry, We can't find this account"
        errormessage+="<p>"+getErrorMessage('error.page.info.unrecognized.json')+"</p>";
        if(accountnumber!=null && accountnumber!=""){
            var validaccountnumber = true;
          accountnumber = accountnumber.replace(/\s/g, "");
            var accNumSubstr = accountnumber.substring(0, 2);
          if((accountnumber.length <12 || accountnumber.length >16) ||
               (accountnumber.length ==12 && (accNumSubstr != '85' && accNumSubstr != '91')) ||
               ((accNumSubstr == '85' || accNumSubstr == '91') && isNaN(accountnumber)&& accountnumber.length ==12)) {
               errormessage+="<ul><li>"+errorLabel+': '+getErrorMessage('error.page.accounts.unmatch.json')+"</li></ul>";//"Customer reference number: This doesn't match any accounts we have stored"
               $('#missingAccountError').html(errormessage).addClass("error").show();
               $("#missingAccount").addClass("error-field");
               validaccountnumber = false;
            }
            else if(checkForSpecialChar(accountnumber)){
                 errormessage+="<ul><li>"+errorLabel+': '+getErrorMessage('error.customerReferenceNumber.invalid.json')+"</li></ul>";
                 $('#missingAccountError').html(errormessage).addClass("error").show();
                 $("#missingAccount").addClass("error-field");
                 validaccountnumber = false;
            }
            else{
                validaccountnumber = true;
                }
            if (validaccountnumber) {
               missingAccountDetailsCall(accountnumber);
            }
       }
       else{
           errormessage+="<ul><li>"+errorLabel+': '+getErrorMessage('error.page.cutomerreferencenumber.not.blank.json')+"</li></ul>";//"Customer reference number cannot be blank"
           $('#missingAccountError').html(errormessage).addClass("error").show();
           $("#missingAccount").addClass("error-field");
       }
   }
   function missingAccountDetailsCall(accountNumber) {
       var title = document.getElementById("title").value;
       var firstName = document.getElementById("firstName").value;
       var surname = document.getElementById("surname").value;
       var customerReferenceNumber = accountNumber;
       var dataString= 'missingAccountNumber=' + accountNumber+'&journey=missingaccounts' +
                       '&title=' + title + '&firstName=' + firstName +  '&surname=' + surname +
                       '&customerReferenceNumber=' + accountNumber;
       $.ajax({
                     type : "POST",
                     url : "/apps/britishgas/components/login/missingAccounts/POST.servlet",
                     data : dataString,
                     async:false,
                     dataType : "json",
                     success : function(response) {
                      var dynamicErrorMessage="";
                        if (typeof response.error != 'undefined') {
                            dynamicErrorMessage= "<h3 class='error-text'>"+getErrorMessage('error.page.no.account.found.json')+"</h3>";//"Sorry, We can't find this account"
                            dynamicErrorMessage+="<p>"+getErrorMessage('error.page.info.unrecognized.json')+"</p>";//We're missing or don't recognise some information:
                            if(response.error.page!=null){
                                 dynamicErrorMessage+="<p>"+response.error.page+"</p>"}
                             else if(response.error.system!=null){
                                 dynamicErrorMessage+="<p>"+response.error.system+"</p>";
                             }
                             else{
                                 dynamicErrorMessage+="<p>"+response.error.errorMessage+"</p>";
                                 }
                             $('#missingAccountError').html(dynamicErrorMessage).addClass("error").show();
                             $("#missingAccount").addClass("error-field");
                         } else {
                              var custAccountsForFreeze=response.missingAccountsList.accountNumberList;
                              var showOverlay=""+response.missingAccountsList.showOverlay;
                              var regEmail=""+response.missingAccountsList.emailAddress;
                              if(custAccountsForFreeze.length>0){
                                  if(showOverlay == "true"){
                                      isFreeze=true;
                                      dynamicErrorMessage="<p>"+getErrorMessage('info.page.account.already.setup.json')+accountNumber+getErrorMessage('info.page.freeze.emailaddress.json')+regEmail+"</p>";
                                      dynamicErrorMessage+="<p>"+getErrorMessage('info.page.freeze.account.text.json')+"</p>";
                                      dynamicErrorMessage+="<p>"+getErrorMessage('info.page.logout.and.login.json')+"</p>";
                                      dynamicErrorMessage+="<p>"+getErrorMessage('info.page.click.missing.product.json')+"</p>";
                                       $('#missingAccountInfo').html(dynamicErrorMessage).show();
                                       $("#missingAccountDiv .customerReferenceNumber").hide();
                                  }
                              else{
                                  $("#missing_account").dialog( "close" );
                                  location.reload(true);
                              }
                              }
                              else{
                              $("#missingAccountDiv .customerReferenceNumber").show();
                              $("#missingAccount").addClass("error-field");
                              var errormessage = "";
                              errormessage= "<h3 class='error-text'>"+getErrorMessage('error.page.no.account.found.json')+"</h3>";
                              $('#missingAccountError').html(errormessage+"<p>"+getErrorMessage('error.page.no.accounts.available.json')+"</p>").addClass("error").show();
                              }
                        }
                     },
                     error: function(xhr){
                         if (xhr.status != 200) {
                          alert('fail');
                         }
                     }
                 });
               return false;
   }
   function freezeAccountDetailsCall() {
       var accountnumberForFreeze = $('#missingAccount').val();
       var title = document.getElementById("title").value;
       var firstName = document.getElementById("firstName").value;
       var surname = document.getElementById("surname").value;
       var customerReferenceNumber = accountnumberForFreeze;
       var dataStringForFreeze= 'missingAccountNumber=' + accountnumberForFreeze+'&journey=missingaccounts' +
       '&title=' + title + '&firstName=' + firstName +  '&surname=' + surname +
       '&customerReferenceNumber=' + accountnumberForFreeze;
         $.ajax({
                       type : "POST",
                       url : "/apps/britishgas/components/login/freeze/POST.servlet",
                       data : dataStringForFreeze,
                       async:false,
                       dataType : "json",
                       success : function(response) {
                       $('#missingAccountError').removeClass('error').html('');
                       $('#missingAccountError, #missingAccountInfo').hide();
                       $("#missingAccount").removeClass("error-field");
             var dynamicErrorMessage="";
                           if (typeof response.error != 'undefined') {
                               dynamicErrorMessage= "<h3 class='error-text'>"+getErrorMessage('error.page.no.account.found.json')+"</h3>";//"Sorry, We can't find this account";
                               if(response.error.page!=null){
                                   dynamicErrorMessage+="<p>"+response.error.page+"</p>";}
                               if(response.error.system!=null){
                                   dynamicErrorMessage+="<p>"+response.error.system+"</p>";
                               }
                               else{
                                   dynamicErrorMessage+="<p>"+response.error.errorMessage+"</p>";
                                   }
                               $('#missingAccountError').html(dynamicErrorMessage).addClass("error").show();
                               $("#missingAccount").addClass("error-field");
                              } else {
                                  var custAccountsForFreeze=response.accountNumberList;
                                  if(custAccountsForFreeze.length>0){
                                    $("#missing_account").dialog( "close" );
                                    location.reload(true);
                                  }
                                  else{
                                      $("#missingAccountDiv .customerReferenceNumber").show();
                                      $("#missingAccount").addClass("error-field");
                                      var errormessage = "";
                                      errormessage= "<h3 class='error-text'>"+getErrorMessage('error.page.no.account.found.json')+"</h3>";
                                      $('#missingAccountError').html(errormessage+"<p>"+getErrorMessage('error.page.no.accounts.available.json')+"</p>").addClass("error").show();
                                  }
                              }                            //The below function is copied from function.js because it needs to be reinitialized after AJAX POST
                       },
                       error: function(xhr){
                           if (xhr.status != 200) {
                              alert('freeze account fails');
                           }
                       }
                   });
                 return false;
   }
function getErrorMessage(fromurlpath){
        var fromrl=urlpath+fromurlpath;
       return getErrorDetails(fromrl);
}
function checkForSpecialChar(accountNumber){
    var charValue=accountNumber;
    var charExp = /^[a-zA-Z0-9]*$/;
    if (accountNumber.match(charExp)){
        return 0;
    }
    else{
        return 1;
    }
}
function redirectToBGMOPage(mpan,mprn,bussinessPartner,duelAddr,nextPageUpgradeSmartMeter,formName,appointmentAvailable) {
if(appointmentAvailable == 'YES'){
        $("#mpanAndAccountNumber").val(mpan);
        $("#mprnAndAccountNumber").val(mprn);
        $("#duelFuelAddress").val(duelAddr);
        $("#bussinessPartner").val(bussinessPartner);
        $("#nextPage").val(nextPageUpgradeSmartMeter);
        $("#"+formName).attr("action","/apps/britishgas/components/channelactivation/smartmeterinstallation/POST.servlet");
        $("#"+formName).submit();
}else{
    openModal('smartmeterupgradeoverlay', 'overlay', true);
}
}
