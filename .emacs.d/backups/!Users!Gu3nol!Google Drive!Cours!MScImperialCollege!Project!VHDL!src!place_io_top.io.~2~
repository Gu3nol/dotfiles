	(pin name="row[6]"	offset=30 layer=2 width=1 depth=1 place_status=placed  )
	(pin name="row[7]"	offset=32 layer=2 width=1 depth=1 place_status=placed  )

    (pin name="anag_mux[0]"	offset=34 layer=2 width=1 depth=1 place_status=placed  )
    (pin name="anag_mux[1]"	offset=36 layer=2 width=1 depth=1 place_status=placed  )
    (pin name="anag_mux[2]"	offset=38 layer=2 width=1 depth=1 place_status=placed  )
    (pin name="anag_mux[3]"	offset=40 layer=2 width=1 depth=1 place_status=placed  )
    (pin name="anag_mux[4]"	offset=42 layer=2 width=1 depth=1 place_status=placed  )
    (pin name="anag_mux[5]"	offset=44 layer=2 width=1 depth=1 place_status=placed  )
    (pin name="anag_mux[6]"	offset=46 layer=2 width=1 depth=1 place_status=placed  )
    (pin name="anag_mux[7]"	offset=48 layer=2 width=1 depth=1 place_status=placed  )
