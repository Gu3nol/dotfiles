    (pin name="anag_mux[11]" offset=56 layer=2 width=1 depth=1 place_status=placed  )
    )

    (right
    (pin name="Mclk" offset=10 layer=1 width=1 depth=1 place_status=placed  )
    (pin name="RDY" offset=20 layer=1 width=1 depth=1 place_status=placed  )

    (pin name="Sclk"	offset=30 layer=1 width=1 depth=1 place_status=placed  )
	(pin name="SC"	offset=40 layer=1 width=1 depth=1 place_status=placed  )
	(pin name="MOSI"	offset=50 layer=1 width=1 depth=1 place_status=placed  )
