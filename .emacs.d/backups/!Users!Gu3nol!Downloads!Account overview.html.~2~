<script>


$(document).ready(function() {
  initiateSessionCounter();
});
var secs;
var sessionTimeOutForm;
var sessionTimer;
var logoutDestiny = '/Logout/Logout-Session-Succesful/';
var redirectDestiny = '/Logout/Logout-Succesful/';

function initiateSessionCounter(){
    sessionTimer = setTimeout("initiate()", 1140000);
}

function resetTimer(){
    stopTimer();
    clearTimeout(sessionTimer);
    initiateSessionCounter();
}

function initiate() {
    openModal('timeoutoverlay-content', 'overlay', true);
    document.getElementById('stopTimer').value = "false";
    countLastMinute(60);
}

function countLastMinute(seconds) {
    var secondsArray;
    var secsString;
    if (seconds != null && seconds != undefined){
        if (seconds == 60) {
            secs = seconds;
        }
    }

    secs--;

    if(secs == -1) {
        secs=0;
    }

    document.getElementById('disp').innerHTML = secs;

    if(secs == 0 && document.sessionTimeOutForm.stopTimer.value == "false") {
        parent.location = logoutDestiny;
    } else {
        if (document.sessionTimeOutForm.stopTimer.value == "false") {
            sessionTimeOutForm = setTimeout("countLastMinute()", 1000);
        }
    }
}

function redo11()
{
var secs=0;

    if(secs == 0)
    {
        parent.location = redirectDestiny; // redirects to specified page once timer ends
    }
}

function stopTimer() {
    document.getElementById('stopTimer').value = "true";
}

$(document).ready(function() {
    $("#keepMeLoggedIn").click(function(){
        keepSessionAlive();
        closeDialog('timeoutoverlay-content');
        resetTimer();
    })
});


function keepSessionAlive()
{
    var xmlhttp=false;
     try {
      xmlhttp = new ActiveXObject("Msxml2.XMLHTTP");
     } catch (e) {
      try {
       xmlhttp = new ActiveXObject("Microsoft.XMLHTTP");
      } catch (E) {
       xmlhttp = false;
      }
     }
    if (!xmlhttp && typeof XMLHttpRequest!='undefined') {
        try {
            xmlhttp = new XMLHttpRequest();
        } catch (e) {
            xmlhttp=false;
        }
    }
    if (!xmlhttp && window.createRequest) {
        try {
            xmlhttp = window.createRequest();
        } catch (e) {
            xmlhttp=false;
        }
    }

     xmlhttp.open("POST", "/OnlineCustomer/jsp/SessionAlive.jsp",true);

     xmlhttp.onreadystatechange=function() {

      if (xmlhttp.readyState==4) {
      }
     }
     xmlhttp.send(null)
}

</script>


