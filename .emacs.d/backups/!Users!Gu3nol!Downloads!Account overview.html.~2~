   var objPoint,prevObj;
   $(window).load(function(){
      _.templateSettings = {
            evaluate:    /\{%(.+?)%\}/g,
            interpolate : /\{\{(.+?)\}\}/g
         };
      if($('#mpllOnlyAccount').val()=='true'){
         $('.loginRewardPoint').hide()
      }
       if($('#multiPremiseLandlord').val()=='true'){
       initialLoad();
       }
     $('.flexslider').flexslider({
        animation: "slide",
        animationLoop: false,
        itemWidth: 75,
        itemMargin: 1,
        minItems: 2,
        maxItems: 6,
        touch: true,
        controlNav: false,
        keyboard: true,
        touch: true,
        slideshow:false,
        directionNav:false,
        mousewheel:true,
        start: function(slider){
          $('body').removeClass('loading');
        }
      });
         $('#singleli').removeClass('ui-tabs-selected ui-state-active') ;
         $('#tab2').removeClass('ui-tabs-hide') ;
         $('#tab1').addClass('ui-tabs-hide');
         $('#multili').addClass('ui-tabs-selected ui-state-active');
        $('#searchButton').click(function(){
            var postcodeRegEx = /^[A-Z]{1,2}[0-9R][0-9A-Z]? ?[0-9][ABD-HJLNP-UW-Z]{2}$/i;
            var houseNumberRegEx = /^[A-Za-z0-9]{1,5}$/i;
            if($.trim($("#maddressLane").val()).length != 0){
                if (!houseNumberRegEx.test($("#maddressLane").val())){
                     var addressInvalid=getErrorMessage('error.houseNumberOrName.blank.json');
                     addressInvalid="<span>"+addressInvalid+"</span>";
                     $("#maddressLane").addClass("error-field");
                     $("#maddressLane").next('span').html(addressInvalid);
                }
            }
            if($.trim($("#mpostCode").val()).length == 0){
                var postcodeEmpty=getErrorMessage('error.diagnostic.tool.blank.postcode.json');
                postcodeEmpty="<span>"+postcodeEmpty+"</span>";
                $("#mpostCode").addClass("error-field");
                $("#mpostCode").next('span').html(postcodeEmpty);

            }else {
               if (!postcodeRegEx.test($("#mpostCode").val())){
                     var postcodeInvalid=getErrorMessage('error.diagnostic.tool.invalid.postcode.json');
                     postcodeInvalid="<span>"+postcodeInvalid+"</span>";
                     $("#mpostCode").addClass("error-field");
                     $("#mpostCode").next('span').html(postcodeInvalid);
                }else{
                     $("#mpostCode,#maddressLane").next('span').html("");
                     $("#mpostCode,#maddressLane").removeClass("error-field");
            $('#panelContent').html("");
            $('#panael-loading').removeClass('displaynone');
            searchContracts();
                }
            }
        });
    });
    function initialLoad(){
        var issuccess=false;
        var milliseconds = new Date().getTime();
       $.ajax({url: "/apps/britishgas/components/LLGetAccountDetails/GET.servlet",type: "GET",async: false, data: "timeStamp="+milliseconds, dataType: "json", success: function(json) {issuccess=true;exvalues = json;}});
      if(issuccess){
          if(device=='phone'){
               demoTemplate = _.template($('#mainTempBlockPhone').html());
                var newhtml = demoTemplate({'contractList' : exvalues});
                $("#panelContent").html(newhtml);
                $('.summary-head-detail .accordion-left-block').click(function(e){
                    if($(this).hasClass('close')){
                         $(this).parent().next('.feature-content').removeClass('displaynone');
                         $(this).addClass('open').removeClass('close');
                         accordclick($(this).attr('id'));
                    }else{
                       $(this).parent().next('.feature-content').addClass('displaynone');
                         $(this).removeClass('open').addClass('close');
                    }
                    $('#bottomContent').find('#backtotop').removeClass('displaynone');
                });
              scrollCheckPhone();
          }else{
                demoTemplate = _.template($('#mainTempBlock').html());
                var newhtml = demoTemplate({'contractList' : exvalues});
                $("#panelContent").html(newhtml);
                $("#panelContent").removeClass('displaynone');
                $("#panael-loading").addClass('displaynone');
                scrollCheckDesktop();
               }
      }else{
          errorMarkup(1, $("#panelContent"))
      }
      issuccess=false;
    }
    function reloadContracts(){
        if(searchPressed){
             $('#panelContent').empty();
            searchPressed=false;
            initialLoad();
        }
    }
     function loadProdcutdeatils(curObject,subProduct){
         setTimeout(function() {
         if($('#'+subProduct).hasClass('plus')){
           $('#'+subProduct).next("#accord").children('.acc_loading').removeClass('displaynone');
         }
          var demoTemplate ;
          var issuccess=false;
           if(device=='phone'){
              var checkPhone=$(curObject).parent(".summary-address-inner").next('.product-block').children('.product-details');

              var productParent;
              if($(curObject).hasClass('accordion-inner-head')){
                  productParent= $(curObject).parent(".summary-address-inner").next('.product-block');
               }else{
                    productParent=$(curObject).next('#subProductPhone')
               }

               if($(curObject).hasClass('open') && checkPhone.length <= 1 && !($(curObject).hasClass('product-added'))){
                  $(productParent).children('.acc_loading').removeClass('displaynone');
                  var accStr='contractReference='+$(curObject).attr('id')
                  $.ajax({url: "/apps/britishgas/components/LLGetProductDetails/GET.servlet?"+accStr,type: "GET", async: false, dataType: "json", success: function(json) {issuccess=true;exvalues = json;}});
                    if(issuccess){
                      demoTemplate = _.template($('#subproductPhone').html());
                      var newhtml = demoTemplate({'item' : exvalues});
                      $(productParent).children('.product-details').html(newhtml);
                      $(productParent).children('.acc_loading').addClass('displaynone');

                       }else{
                        errorMarkup(3,productParent)
                  }
                     $(curObject).addClass('product-added');
                 }

               if($(curObject).hasClass('product-added')){
                $(curObject).next("#accord").children('.acc_loading').addClass('displaynone');
              }

           }else{
                var check= $(curObject).next("#accord").children(".product-details");
               if($(curObject).hasClass('minus') && check.length <= 1 && !($(curObject).hasClass('product-added'))){
                  var accStr='contractReference='+$(curObject).attr('id');
                  $.ajax({url: "/apps/britishgas/components/LLGetProductDetails/GET.servlet?"+accStr,type: "GET", async: false, dataType: "json", success: function(json) {issuccess=true;exvalues = json;}});
                   if(issuccess){
                      demoTemplate = _.template($('#subTempBlock').html());
                      var newhtml = demoTemplate({'item' : exvalues});
                      $(curObject).next("#accord").children(".product-details").html(newhtml);
                      $(curObject).next("#accord").children('.acc_loading').addClass('displaynone');
                   }else{
                         errorMarkup(3,$(curObject).next("#accord"))
                   }
                   $(curObject).addClass('product-added');
               }
               if($(curObject).hasClass('product-added')){
                   $(curObject).next("#accord").children('.acc_loading').addClass('displaynone');
               }
            }
        issuccess=false
         }, 500);
      }
    function isScrolledIntoView(elem)
    {
        var docViewTop = $(window).scrollTop();
        var docViewBottom = docViewTop + $(window).height();
        var elemTop = $(elem).offset().top;
        var elemBottom = elemTop + $(elem).height();
        return ((elemBottom <= docViewBottom) && (elemTop >= docViewTop));
    }
    function accordclick(ide,contractStatus)
    {
        var curobj=$('#'+ide);
        var a=$(curobj).attr('id')
        viewProperties(curobj,ide,contractStatus)
    }
    var onscrollOBj;
    function scrollCheckPhone(){
        $(window).bind("scroll", function() {
        if(onscrollOBj!=null){
            var checking=isScrolledIntoView(onscrollOBj);
            var  parentObj=$(onscrollOBj).parent();
            //if last scroll is not done, don't start another one.
            var childObj=$(parentObj).children('div.summary-address');
            var loaderParent=$(parentObj).nextAll('.summary-address')
            var loader=$(loaderParent).find('#ViewAddressLoading');
            if(checking && childObj.length<$(parentObj).attr('prev') && $(parentObj).css('display')=='block'){
                   $(loader).addClass('displayblock');
                            demoTemplate = _.template($('#TempBlockPhone').html());
                            var issuccess=false;
                            contentId = $(parentObj).before().attr('id');
                            contractNum = $("#"+contentId).attr("id").replace("subAc","");
                            currentPosit=childObj.length+1;
                            queryStr='accountNumber='+contractNum+'&currentPosition='+currentPosit+'&range='+rangeofPaginate;
                         $.ajax({url: "/apps/britishgas/components/LLGetMultiPremiseDetails/GET.servlet?"+queryStr,type:"GET", async: false, dataType: "json", success: function(json) {issuccess=true;exvalues = json;}});
                        if(issuccess){
                            var oldhtml=$(parentObj).html();
                            var newhtml = oldhtml+demoTemplate({'propertiesList' : exvalues});
                            $(parentObj).html(newhtml);
                            onscrollOBj=$(parentObj).children('div.summary-address:last');
                            isScrolledIntoView(onscrollOBj);
                            $(".summary-address-inner .accordion-inner-head").removeClass('open');
                            $(".summary-address-inner .accordion-inner-head").addClass('close');
                            $('.summary-address-inner .accordion-inner-head').unbind("click");
                            $('.summary-address-inner .accordion-inner-head').click(function(e){
                                loadProdcutdeatils(this)
                                $(this).toggleClass('close').toggleClass('open');
                                $(this).parents('.summary-address').find("#subProductPhone").slideToggle(function (){
                                $(this).parents('.summary-address').find("#subProductPhone").toggleClass('active', $(this).parents('.summary-address').find("#subProductPhone").is(':visible'));
                                });
                            });
                       }else{
                            $('#panael-loading').addClass('displaynone');
                            $('.error-message').html(getErrorMessage('error.mpll.no.records.available.json'));
                            $('.error-text').html(getErrorMessage('error.mpll.system.header.json'));
                            $('.error').show();
                       }
                        scollLoadTimer=null;
                        $(loader).removeClass('displayblock');
                        $(loader).addClass('displaynone')
                        issuccess=false;
                 }
            }
       });
    }
    var searchPressed=false;
    function searchContracts(){
          $('.error').hide();
           quertStr='postCode='+$('#mpostCode').val()+'&addressLine='+$('#maddressLane').val();
           var issuccess=false;
           searchPressed=true;
          $.ajax({url: "/apps/britishgas/components/LLGetMultiPremiseDetails/GET.servlet?"+quertStr, async: false, dataType: "json", success: function(json) {issuccess=true;exvalues = json;}});
        if(issuccess && exvalues && exvalues.premises){
                    $('#panael-loading').addClass('displaynone');
                      if(device=='phone'){
                        demoTemplate = _.template($('#TempBlockPhone').html());
                        var newhtml = demoTemplate({'propertiesList' : exvalues});
                        $('#panelContent').empty();
                        $('#panelContent').html(newhtml);
                        $('.summary-address-inner .accordion-inner-head').click(function(e){
                            loadProdcutdeatils(this);
                            $(this).toggleClass('close').toggleClass('open');
                            $(this).parents('.summary-address').find("#subProductPhone").slideToggle(function (){
                            $(this).parents('.summary-address').find("#subProductPhone").toggleClass('active', $(this).parents('.summary-address').find("#subProductPhone").is(':visible'));
                            });
                        });
                }else{
                    demoTemplate = _.template($('#TempBlock').html());
                    var newhtml = demoTemplate({'propertiesList' : exvalues});
                    $("#panelContent").empty();
                    $("#panelContent").html(newhtml);
                    var target = $('#searchButton');
                    $('html,body').animate({
                      scrollTop: target.offset().top
                    }, 1000);
                }
        }else{
            $('#panael-loading').addClass('displaynone');
                $('.error-message').html(getErrorMessage('error.mpll.no.records.available.json'));
                $('.error-text').html(getErrorMessage('error.mpll.system.header.json'));
                $('.error').show();
            }
        issuccess=false;
        }
    function viewProperties(curobj,id,contractStatus){
        $("#"+id).next('div').next().find('.acc_loading').removeClass('displaynone');
        var loader=$("#"+id).next('#bottomContent').find('#ViewAccountLoading');
        var noOfProperties=$("#no-ofproperties").val();
        if(device=='phone'){
            if(!($("#"+id).hasClass('producd-loaded'))){
            demoTemplate = _.template($('#TempBlockPhone').html());
            var issuccess=false;
             var querStr='accountNumber='+$(curobj).attr('id').replace('Ac','')+'&currentPosition=1&range='+rangeofPaginate;
                $.ajax({url: "/apps/britishgas/components/LLGetMultiPremiseDetails/GET.servlet?"+querStr,type:"GET", async: false, dataType: "json", success: function(json) {issuccess=true;exvalues = json;}});
           if(issuccess && exvalues ){
                exvalues.businessContractNumber=$(curobj).attr('id').replace('Ac','');
                $.each(exvalues.premises,function(key,value){
                      if($("#"+id).hasClass("Laspsed") || $("#"+id).hasClass("Cancelled")){
                         value.bookRepairAvailable = false;
                         value.asvCP12Available=false;
                      }
                 });

                var newhtml = demoTemplate({'propertiesList' : exvalues});
                $("#sub"+id).html(newhtml);
                onscrollOBj=$("#sub"+id).children('div.summary-address:last');
                isScrolledIntoView(onscrollOBj);
                $('.summary-address-inner .accordion-inner-head').click(function(e){
                    $(this).toggleClass('close').toggleClass('open');
                    $(this).parents('.summary-address').find("#subProductPhone").slideToggle(function (){
                    $(this).parents('.summary-address').find("#subProductPhone").toggleClass('active', $(this).parents('.summary-address').find("#subProductPhone").is(':visible'));
                    });
                     loadProdcutdeatils(this)
                });
                $("#"+id).parent('div').next(".feature-content").find(".acc_loading").addClass("displaynone");
                $("#"+id).addClass("producd-loaded");
            }else{
                    $('#panael-loading').addClass('displaynone');
                    $('.error-message').html(getErrorMessage('error.mpll.no.records.available.json'));
                    $('.error-text').html(getErrorMessage('error.mpll.system.header.json'));
                    $('.error').show();
            }
            issuccess=false;
          }
        }else{
            var childlenth=$(curobj).next("#acordionBlock").children('div.panel-content');
            if($(curobj).next("#acordionBlock").css('display')=='none' && childlenth.length<=0){
               setTimeout(function() {
                    demoTemplate = _.template($('#TempBlock').html());
                     var issuccess=false;
                     var querStr='accountNumber='+$(curobj).attr('id').replace('Ac','')+'&currentPosition=1&range='+rangeofPaginate+'&contractStatus='+contractStatus;
                    $.ajax({url: "/apps/britishgas/components/LLGetMultiPremiseDetails/GET.servlet?"+querStr,type:"GET", async: false, dataType: "json", success: function(json) {issuccess=true;exvalues = json;}});
                    if(issuccess && exvalues){
                        setBusinessContractNumber(exvalues,$(curobj).attr('id').replace('Ac',''));
                        exvalues.businessContractNumber=$(curobj).attr('id').replace('Ac','');
                        $.each(exvalues.premises,function(key,value) {
                          if(contractStatus == 'Lapsed' || contractStatus == 'Cancelled' )
                          {
                             value.bookRepairAvailable = false;
                             value.asvCP12Available=false;
                          }
                        });
                        var newhtml = demoTemplate({'propertiesList' : exvalues});
                        $(curobj).next("#acordionBlock").html(newhtml);
                        var objcur=$(curobj).next("#acordionBlock")
                        var lastObj=$(objcur).children('div.panel-content:last');
                        $("#"+id).next('div').next().find('.acc_loading').addClass('displaynone');
                        onscrollOBj=$(lastObj);
                        isScrolledIntoView(onscrollOBj);
                  }else{
                        $('#panael-loading').addClass('displaynone');
                        $('.error-message').html(getErrorMessage('error.mpll.no.records.available.json'));
                        $('.error-text').html(getErrorMessage('error.mpll.system.header.json'));
                        $('.error').show();
                  }
                  issuccess=false;
                }, 500);
                if($("#"+id).hasClass('plus')){
                    $("#"+id).next('div').next().find('.backtotop').removeClass('displaynone');
                }
            }else{
                if($("#"+id).hasClass('minus')){
                    $("#"+id).next('div').next().find('.backtotop').addClass('displaynone');
                }else{
                    $("#"+id).next('div').next().find('.backtotop').removeClass('displaynone');
                }
                $("#"+id).next('div').next().find('.acc_loading').addClass('displaynone');
            }
        }
      }
   function errorMarkup(erroType,parentObj){
   var authContent;
       switch(erroType){
           case 1:
             authContent=''
           break;
           case 2:
               authContent=''
           break;
           case 3:
               authContent=''
           break;
       }
       var a='<div class="error" class="displayblock"><h3 class="error-text">'+authContent+'</h3></div>';
       $(parentObj).html(a);
   }
    var scollLoadTimer;
    function scrollCheckDesktop(){
        $(window).bind("scroll", function(event) {

        //if last scroll is not done, don't start another one.
        if ($(window).data('ajaxready') === false)
        return;
        if ($(window).scrollTop() >= $(document).height() - $(window).height() - 100) {
        $(window).data('ajaxready', false);  //prevent more scroll events

        if(onscrollOBj!=null){
            var checking=isScrolledIntoView(onscrollOBj);
            var  parentObj=$(onscrollOBj).parent('#acordionBlock')
            var childObj=$(parentObj).children('div.panel-content');
            var loader=$(parentObj).next('#bottomContent').find('#ViewAccountLoading')
            if(checking && childObj.length<$(parentObj).attr('prev') && $(parentObj).css('display')=='block'){
                    var parentId=$(parentObj).attr('id')
                    demoTemplate = _.template($('#TempBlock').html());
                    var issuccess=false;
                    contentId = $(parentObj).before().attr('id');
                    contractNum = $("#"+contentId).parent("div").children("p").attr("id").replace("Ac","");
                    currentPosit=childObj.length+1;
                    queryStr='accountNumber='+contractNum+'&currentPosition='+currentPosit+'&range='+rangeofPaginate;
                    $('#Ac'+contractNum).next('div').next().find('.acc_loading').removeClass('displaynone');
                    $.ajax({url: "/apps/britishgas/components/LLGetMultiPremiseDetails/GET.servlet?"+queryStr,type:"GET", async: false, dataType: "json", success: function(json) {issuccess=true;exvalues = json;}});
                    if(issuccess){
                        event.preventDefault();
                        var oldhtml=$(parentObj).html();
                        var newhtml =oldhtml+demoTemplate({'propertiesList' : exvalues});
                        $(parentObj).html(newhtml);
                        var lastObj=$(parentObj).children('div.panel-content:last')
                        onscrollOBj=$(lastObj);
                        isScrolledIntoView(onscrollOBj);
                        $('#Ac'+contractNum).next('div').next().find('.backtotop').removeClass('displaynone');
                        $('#Ac'+contractNum).next('div').next().find('.acc_loading').removeClass('displaynone');
                    }
                    else{
                        $('#panael-loading').addClass('displaynone');
                        $('.error-message').html(getErrorMessage('error.mpll.no.records.available.json'));
                        $('.error-text').html(getErrorMessage('error.mpll.system.header.json'));
                        $('.error').show();
                    }
                    issuccess=false;
                    $('#Ac'+contractNum).next('div').next().find('.acc_loading').addClass('displaynone');
            }
          }

          }
    //turn scrolling events back on
          $(window).data('ajaxready', true);

       });

    }
    (function( $ ){
        $.fn.gparent = function( recursion ){
            if( recursion == undefined ) recursion = 2;
            if(typeof(recursion) == "number"){
                recursion = parseInt( recursion );
                if( recursion > 0 ){
                    gparent_holder = $(this);
                    for(var gparent_i = 0; gparent_i < recursion; gparent_i++){
                        gparent_holder = gparent_holder.parent();
                    }
                    return gparent_holder;
                }
                else return false;
            }
            else return false;
        }
        })( jQuery );
    function backtotopfun(obj){
     if(device=='phone'){
        var parent=$(obj).gparent(7);
        var elemTop = $(parent).offset().top;
     }else{
        var parent=$(obj).gparent(7);
        var elemTop = $(parent).offset().top;
     }
         window.scrollTo(0,elemTop)
    }
function doCorrespondingFunc(contractNumber,option,businessContractNumber,contractReference,multiPremiseLandlord,address)
 {
    var postcode=address.split(",");
    postcode=postcode[postcode.length-1].trim();
   var opt = new Array();
   opt['repair']='/apps/britishgas/components/breakdown/multiPremise/IdentifyFault/responsive/GET.servlet?accountType=Home Services&accountNumber='+contractNumber+'&legacyAccountNumber='+contractNumber+'&contractReference='+contractReference+'&isMultiPremiseLandlordCustomer='+multiPremiseLandlord+'&postCode='+postcode;
   opt['ASV']='/apps/britishgas/components/BookingAppointmentSlots/responsive/GET.servlet?accountType=Home Services&legacyAccountNumber='+contractNumber+'&accountNumber='+contractReference+'&isMultiPremiseLandlordCustomer='+multiPremiseLandlord + '&contractReferenceNumber='+contractReference;
   opt['trackJob']='/MultiPremise/Your-Jobs/';
   opt['viewGasCertificate']='/MultiPremise/Gas-Safety-Status/';
   opt['cp12']='/apps/britishgas/components/LLViewYourGasSafetyCertificate/GET.servlet?selectedAccountNumber='+contractNumber;
   opt['correspondance']='/MultiPremise/View-Documents/';
   window.location.href=opt[$('#'+option).val()];
   return false;
}
function setBusinessContractNumber(values , number1)
{
  $.each(values.premises,function(index, value ){
    value.aggrementNumber = number1;
  });
  return values ;
}
function changeselect(obj){
 var tariffId= obj.replace('tariffId', '');
 var goid="#contgoId"+tariffId;
 var selectval= $("#"+obj).attr('selectedIndex');
 if(selectval !=0){
     var goid ="#contgoId"+tariffId;
     $(goid).addClass('btncomp-primary');
     $(goid).removeClass('btncomp-inactive');
     $(goid).removeAttr("disabled","disabled");
 }
 else if(selectval == 0){
     $(goid).attr("disabled","disabled");
     $(goid).addClass('btncomp-inactive');
     $(goid).addClass(' btncomp-deviceinactive');
     $(goid).removeClass('btncomp-primary');
 }
}
 function manageContract(contarct){
   var url = '/apps/britishgas/components/LLContractSummary/GET.servlet?accountNumber='+contarct;
   document.location.href= url;
 }
</script>

