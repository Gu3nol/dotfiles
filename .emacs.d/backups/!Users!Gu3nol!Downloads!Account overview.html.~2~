<script type="text/javascript">
/* Cookie Handling Starts*/
function setCookie(szName, szValue, szExpires, szPath, szDomain, bSecure) {
    var szCookieText = escape(szName) + '=' + escape(szValue);
    szCookieText += (szExpires ? '; EXPIRES=' + szExpires.toGMTString() : '');
    szCookieText += (szPath ? '; PATH=' + szPath : '');
    szCookieText += (szDomain ? '; DOMAIN=' + szDomain : '');
    szCookieText += (bSecure ? '; SECURE' : '');
    document.cookie = szCookieText;
}

function getCookie(szName) {
    var szValue = '';
    if (document.cookie) //only if exists
    {
        var arr = document.cookie.split((escape(szName) + '='));
        if (2 <= arr.length) {
            var arr2 = arr[1].split(';');
            szValue = unescape(arr2[0]);
        }
    }
    return szValue;
}

function deleteCookie(szName) {
    var tmp = getCookie(szName);
    if (tmp) {
        setCookie(szName, '', (new Date(1)), '/');
    }
}

function overWriteCookie(newEmail) {
    var newmail = document.getElementById(newEmail).value;
    COOKIENAME = "centrica-user-email";
    REMEMBER = "REMEMBERCHK";
    var chkVal = getCookie(REMEMBER);
    if (chkVal == 'true') {
        var date = new Date();
        date.setTime(date.getTime() + (10 * 24 * 60 * 60 * 1000));
        deleteCookie(COOKIENAME);
        setCookie(COOKIENAME, newmail, date, '/');
    }
}
/* Cookie Handling Ends*/
</script>





