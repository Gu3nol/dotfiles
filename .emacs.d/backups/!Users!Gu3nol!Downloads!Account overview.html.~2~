<script>
CQ_Analytics.ClickstreamcloudMgr.addListener("storesloaded", function(e) {
    if (CQ_Analytics.mboxes) {
        for (var i=0;i<CQ_Analytics.mboxes.length;i++) {
            var updateArgs = [CQ_Analytics.mboxes[i].name];
            //clickstreamcloud mappings
            for(var j=0; j < CQ_Analytics.mboxes[i].mappings.length; j++) {
                var mapping = CQ_Analytics.mboxes[i].mappings[j];
                var tmp = mapping.split(".");
                var mgr = tmp[0];
                var key = tmp[1];
                var manager = {
                    'profile': 'ProfileDataMgr',
                    'customerdata': 'CustomerDataMgr',
                    'bnadata': 'BnaDataMgr',
                    'pagedata': 'PageDataMgr',
                    'tagcloud': 'TagCloudMgr',
                    'surferinfo': 'SurferInfoMgr',
                    'eventdata': 'EventDataMgr'
                };
                if(CQ_Analytics[manager[mgr]] != undefined) {
                    updateArgs.push(mapping + "=" + CQ_Analytics[manager[mgr]].data[key]);
                }
            }
            mboxUpdate.apply(this, updateArgs);
        }
    }
});
</script>


